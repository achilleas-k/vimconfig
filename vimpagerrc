call plug#begin('~/.vim/plugged')
Plug 'nanotech/jellybeans.vim'
call plug#end()

" Colours and GUI {{{
set t_Co=256
colorscheme jellybeans
" }}}

set hlsearch
set ignorecase
set smartcase

let vimpager_passthrough = 1
let vimpager_disable_x11 = 1
let vimpager_disable_ansiesc = 1

" vim:fdm=marker ft=vim
